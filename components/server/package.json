{
  "name": "apollo-server-typescript-boilerplate",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "author": "",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "engines": {
    "node": ">=8.9"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "fix:prettier": "prettier \"src/**/*.ts\" --write --loglevel warn",
    "generate": "graphql-codegen --config codegen.ts",
    "compile": "npm run generate && tsc",
    "start": "dotenv -e .env node ./dist/index.js",
    "start-dev": "npm run compile && dotenv -e .env.dev node ./dist/index.js",
    "watch": "concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\"  \"tsc -w \" \"dotenv -e .env.dev nodemon ./dist/index.js \"",
    "generate:types": "graphql-codegen --config codegen.yml",
    "start-db": "dotenv -e .env.dev npm run docker:up && dotenv -e .env.dev npx prisma migrate dev --preview-feature --skip-generate --name \"init\" && dotenv -e .env.dev npx prisma generate",
    "migrate": "dotenv -e .env.dev npx prisma migrate dev --preview-feature --skip-generate --name \"init\" && dotenv -e .env.dev npx prisma generate",
    "migrate-prod": "dotenv -e .env node node_modules/prisma/build/index.js migrate deploy",
    "prisma-studio": "dotenv -e .env.dev npx prisma studio",
    "test:types": "tsc -p . --noEmit",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "client": "npm run start --prefix client",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start-test-db": "dotenv -e .env.test npm run docker:up && sh -c \"sleep 3;\"",
    "setup-integration-tests": "npm run start-test-db && dotenv -e .env.test npx prisma migrate dev --name \"init\" && npx prisma generate",
    "integration-test": "dotenv -e .env.test jest -- ./src/tests --runInBand --verbose",
    "unit-tests": "dotenv -e .env.test jest -- ./src/services/tests --runInBand --verbose",
    "resolver-tests": "dotenv -e .env.test jest -- ./src/resolvers/tests --runInBand --verbose",
    "destroy": "npm run docker:down",
    "build": "tsc",
    "postbuild": "copyfiles package*.json dist",
    "seed-db": "dotenv -e .env.dev npx prisma db seed",
    "test-query": "dotenv -e .env.dev npx ts-node ./src/test.ts",
    "test": "dotenv -e .env.dev npx ts-node ./src/test.ts"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.171.0",
    "@onesignal/node-onesignal": "^1.0.0-beta4",
    "@prisma/client": "^4.3.0",
    "apollo-datasource": "^3.3.2",
    "apollo-server": "^3.10.2",
    "apollo-server-core": "^3.10.2",
    "apollo-server-lambda": "^3.10.2",
    "apollo-server-plugin-base": "^3.6.2",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "concurrently": "^7.3.0",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-shield": "^7.6.4",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.8",
    "pg": "^8.7.3",
    "save": "^2.5.0",
    "save-dev": "^0.0.1-security",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-operations": "3.0.0",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@graphql-codegen/typescript-resolvers": "3.0.0",
    "@graphql-tools/merge": "^8.2.10",
    "@graphql-tools/schema": "^9.0.4",
    "@graphql-tools/utils": "^8.12.0",
    "@playlyfe/gql": "^2.6.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^12.7.3",
    "@types/nodemailer": "^6.4.5",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "bunyan-prettystream": "^0.1.3",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "graphql-tools": "^8.3.6",
    "husky": "^3.0.5",
    "jest-mock-extended": "^3.0.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.3.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
